@model IEnumerable<AllArticlesServiceModel>
@{
    ViewData["Title"] = "Blog";
}

<h2>@ViewData["Title"] </h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Title</th>
            <th>Content</th>
            <th>Publish date</th>
            <th>Author</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var article in Model)
        {
            <tr>
                <td>@article.Title</td>
                <td>@article.Content</td>
                <td>@article.PublishDate</td>
                <td>@article.Author</td>
            </tr>
        }
    </tbody>
</table>